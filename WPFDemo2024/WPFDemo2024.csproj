<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>media\stripicon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="media\stripicon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Main.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Inventory.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="AreaTwo.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Page Update="Main.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Inventory.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="AreaTwo.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
